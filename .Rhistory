versoes <- list.files(pattern = "dynamic_sampler_v.*\\.R")
versoes <- sort(versoes) # Ordenar
# Função para comparar duas versões
comparar_versoes <- function(v1, v2) {
conteudo1 <- readLines(v1)
conteudo2 <- readLines(v2)
differences <- setdiff(conteudo1, conteudo2)
return(differences)
}
